PACKAGE com.softicar.platform.core.module.ajax.exception

QUERY AjaxExceptionsQuery {

	SELECT exception.exceptionDate AS date
	SELECT exception.user AS user
	SELECT exception.exceptionType AS type
	SELECT exception.exceptionText AS text
	SELECT exception.* AS ajaxException
	SELECT exception.session AS session

	FROM AGAjaxException exception

	ORDER BY exception.id DESC
}
